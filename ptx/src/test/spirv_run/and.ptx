.version 6.5
.target sm_30
.address_size 64

.visible .entry and(
	.param .u64 input,
	.param .u64 output
)
{
	.reg .u64 	    in_addr;
    .reg .u64 	    out_addr;
    .reg .u32 	    temp1;
    .reg .u32 	    temp2;

	ld.param.u64 	in_addr, [input];
    ld.param.u64 	out_addr, [output];

    ld.u32          temp1, [in_addr];
    ld.u32          temp2, [in_addr+4];
	and.b32		    temp1, temp1, temp2;
    st.u32          [out_addr], temp1;
	ret;
}
