.version 6.5
.target sm_30
.address_size 64

.visible .entry block(
	.param .u64 input,
	.param .u64 output
)
{
	.reg .u64 	    in_addr;
    .reg .u64 	    out_addr;
    .reg .u64 	    temp;
    .reg .u64 	    temp2;

	ld.param.u64 	in_addr, [input];
    ld.param.u64 	out_addr, [output];

    ld.u64          temp, [in_addr];
	add.u64		    temp2, temp, 1;
    {
        .reg .u64 	    temp2;
	    add.u64		    temp2, temp2, 1;
    }
    st.u64          [out_addr], temp2;
	ret;
}
