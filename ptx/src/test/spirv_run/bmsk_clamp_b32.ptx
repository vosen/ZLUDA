.version 7.6
.target sm_70
.address_size 64

.visible .entry bmsk_clamp_b32(
	.param .u64 input,
	.param .u64 output
)
{
	.reg .u64 	    in_addr;
    .reg .u64 	    out_addr;
    .reg .u32       position;
    .reg .u32       width;
    .reg .u32       result;

	ld.param.u64 	in_addr, [input];
    ld.param.u64 	out_addr, [output];

    ld.u32          position, [in_addr];
	ld.u32          width, [in_addr+4];
	bmsk.clamp.b32  result, position, width;
    st.u32          [out_addr], result;
	ret;
}
