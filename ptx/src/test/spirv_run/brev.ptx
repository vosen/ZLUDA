.version 6.5
.target sm_30
.address_size 64

.visible .entry brev(
	.param .u64 input,
	.param .u64 output
)
{
	.reg .u64 	    in_addr;
    .reg .u64 	    out_addr;
    .reg .b32       temp;

	ld.param.u64 	in_addr, [input];
    ld.param.u64 	out_addr, [output];

    ld.b32          temp, [in_addr];
	brev.b32        temp, temp;
    st.b32          [out_addr], temp;
	ret;
}
