.version 6.5
.target sm_30
.address_size 64

.const .u32 x = 40;
.const .u32 constparams[4] = { 10, 20, 30, x };

.visible .entry const_ident(
	.param .u64 input,
	.param .u64 output
)
{
	.reg .u64 	    in_addr;
    .reg .u64 	    out_addr;
    .reg .u32 	    temp;

	ld.param.u64 	in_addr, [input];
    ld.param.u64 	out_addr, [output];

    ld.const.u32    temp, [constparams+12];
    st.u32          [out_addr], temp;
	ret;
}
