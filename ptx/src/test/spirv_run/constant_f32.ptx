.version 6.5
.target sm_30
.address_size 64

.visible .entry constant_f32(
	.param .u64 input,
	.param .u64 output
)
{
	.reg .u64 	    in_addr;
    .reg .u64 	    out_addr;
    .reg .f32 	    temp;

	ld.param.u64 	in_addr, [input];
    ld.param.u64 	out_addr, [output];

    ld.f32          temp, [in_addr];
	mul.f32		    temp, temp, 0f3f000000; // 0.5
    st.f32          [out_addr], temp;
	ret;
}
