.version 6.5
.target sm_30
.address_size 64

.visible .entry cvt_sat_s_u(
	.param .u64 input,
	.param .u64 output
)
{
	.reg .u64 	    in_addr;
    .reg .u64 	    out_addr;
    .reg .s32 	    temp;
    .reg .u32       temp2;
    .reg .s32 	    temp3;

	ld.param.u64 	in_addr, [input];
    ld.param.u64 	out_addr, [output];

    ld.s32          temp, [in_addr];
	cvt.sat.u32.s32	temp2, temp;
	cvt.s32.u32		temp3, temp2;
    st.s32          [out_addr], temp3;
	ret;
}
