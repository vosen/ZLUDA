.version 6.5
.target sm_30
.address_size 64

.visible .entry cvta(
	.param .u64 input,
	.param .u64 output
)
{
	.reg .u64 	        in_addr;
    .reg .u64 	        out_addr;
    .reg .f32 	        temp;

	ld.param.u64 	    in_addr, [input];
    ld.param.u64 	    out_addr, [output];

    cvta.to.global.u64  in_addr, in_addr;
    cvta.to.global.u64  out_addr, out_addr;

    ld.global.f32       temp, [in_addr];
    st.global.f32       [out_addr], temp;
	ret;
}
