.version 6.5
.target sm_30
.address_size 64

.global .f32 foobar[4] = {0f3f800000};

.visible .entry global_array_f32(
	.param .u64 input,
	.param .u64 output
)
{
	.reg .u64 	        in_addr;
    .reg .u64 	        out_addr;
    .reg .f32 	        temp;

	mov.u64 	        in_addr, foobar;
    ld.param.u64 	    out_addr, [output];

    ld.global.f32       temp, [in_addr+4];
    st.f32              [out_addr], temp;
	ret;
}