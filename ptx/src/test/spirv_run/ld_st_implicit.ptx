.version 6.5
.target sm_30
.address_size 64

.visible .entry ld_st_implicit(
	.param .u64 input,
	.param .u64 output
)
{
	.reg .u64 	    in_addr;
    .reg .u64 	    out_addr;
    .reg .b64 	    temp;

	ld.param.u64 	in_addr, [input];
    ld.param.u64 	out_addr, [output];

    mov.b64                 temp, 0x0123456789abcdef;
    ld.global.f32           temp, [in_addr];
    st.global.f32           [out_addr], temp;
	ret;
}
