.version 7.0
.target sm_80
.address_size 64

.visible .entry min_f16(
    .param .u64 input,
    .param .u64 output
)
{
    .reg .u64       in_addr;
    .reg .u64       out_addr;
    .reg .f16       temp1;
    .reg .f16       temp2;

    ld.param.u64    in_addr, [input];
    ld.param.u64    out_addr, [output];

    ld.b16          temp1, [in_addr];
    ld.b16          temp2, [in_addr+2];
    min.f16         temp1, temp1, temp2;
    st.b16          [out_addr], temp1;
    ret;
}
