.version 6.5
.target sm_30
.address_size 64

.visible .entry param_is_addressable(
	.param .u64 input,
	.param .u64 output
)
{
	.reg .u64 	    in_addr;
    .reg .u64 	    out_addr;
    .reg .b64 	    temp;

	ld.param.u64 	in_addr, [input];
    ld.param.u64 	out_addr, [output];

    mov.b64         temp, input;
    ld.param.b64    temp, [temp];
    sub.u64         temp, temp, in_addr;
    st.u64          [out_addr], temp;
	ret;
}
