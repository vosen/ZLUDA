.version 6.5
.target sm_30
.address_size 64

.visible .entry prmt(
	.param .u64 input,
	.param .u64 output
)
{
	.reg .u64 	    in_addr;
    .reg .u64 	    out_addr;
    .reg .u32 	    temp1;
    .reg .u32 	    temp2;

	ld.param.u64 	in_addr, [input];
    ld.param.u64 	out_addr, [output];

    ld.u32          temp1, [in_addr];
    ld.u32          temp2, [in_addr+4];
	prmt.b32        temp2, temp1, temp2, 30212;
    st.u32          [out_addr], temp2;
	ret;
}
