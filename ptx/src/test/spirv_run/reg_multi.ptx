.version 6.5
.target sm_30
.address_size 64

.visible .entry reg_multi(
	.param .u64 input,
	.param .u64 output
)
{
	.reg .u64 	    in_addr;
    .reg .u64 	    out_addr;
    .reg .u32       a, b;

	ld.param.u64 	in_addr, [input];
    ld.param.u64 	out_addr, [output];

    ld.u32   a, [in_addr];
    ld.u32   b, [in_addr+4];
    st.u32   [out_addr], a;
    st.u32   [out_addr+4], b;
	ret;
}
