.version 6.5
.target sm_30
.address_size 64

.visible .entry sad_s64(
	.param .u64 input,
	.param .u64 output
)
{
	.reg .u64 	    in_addr;
    .reg .u64 	    out_addr;
    .reg .s64 	    temp1;
    .reg .s64 	    temp2;
    .reg .s64 	    temp3;
    .reg .s64 	    result1;
    .reg .s64 	    result2;

	ld.param.u64 	in_addr, [input];
    ld.param.u64 	out_addr, [output];

    ld.s64          temp1, [in_addr];
    ld.s64          temp2, [in_addr + 8];
    ld.s64          temp3, [in_addr + 16];
    sad.s64   		result1, temp1, temp2, temp3;
    sad.s64   		result2, temp2, temp1, temp3;
    st.s64          [out_addr], result1;
    st.s64          [out_addr+8], result2;
	ret;
}
