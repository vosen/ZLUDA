.version 6.5
.target sm_30
.address_size 64

.visible .entry sign_extend(
	.param .u64 input,
	.param .u64 output
)
{
	.reg .u64 	    in_addr;
    .reg .u64 	    out_addr;
    .reg .s32 	    temp;

	ld.param.u64 	in_addr, [input];
    ld.param.u64 	out_addr, [output];

    ld.s16          temp, [in_addr];
    st.s32          [out_addr], temp;
	ret;
}
