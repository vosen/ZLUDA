.version 6.5
.target sm_30
.address_size 64

.visible .entry sqrt_rn_ftz(
	.param .u64 input,
	.param .u64 output
)
{
	.reg .u64 	    in_addr;
    .reg .u64 	    out_addr;
    .reg .f32 	    temp1;

	ld.param.u64 	in_addr, [input];
    ld.param.u64 	out_addr, [output];

    ld.f32          temp1, [in_addr];
	sqrt.rn.ftz.f32	temp1, temp1;
    st.f32          [out_addr], temp1;
	ret;
}
