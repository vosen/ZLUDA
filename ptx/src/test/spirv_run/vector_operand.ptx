.version 6.5
.target sm_60
.address_size 64

.visible .entry vector_operand(
    .param .u64 input_p,
    .param .u64 output_p
)
{
    .reg .u64    in_addr;
    .reg .u64    out_addr;

    .reg .b16    in;
    .reg .b32    out;

    ld.param.u64 in_addr, [input_p];
    ld.param.u64 out_addr, [output_p];

    ld.b16       in, [in_addr];

    mov.b32      out, {0x5678, in};

    st.b32       [out_addr], out;
    ret;
}